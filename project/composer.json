{
    "name": "someson/phalcon5-docker",
    "description": "dockerized application based on phalcon framework 5.x",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "7.4.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phalcon/ide-stubs": "v5.0.0-beta.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
