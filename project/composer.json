{
    "name": "someson/phalcon5-docker",
    "description": "dockerized application based on phalcon framework 5.x",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "8.0.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phalcon/ide-stubs": "v5.0.0-beta.3",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-db": "^1.2",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-phalcon5": "^1.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.4",
        "codeception/module-webdriver": "^1.4",
        "codeception/verify": "^0.4",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        }
    },
    "minimum-stability": "dev"
}
